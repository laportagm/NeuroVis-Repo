{
  "godot-mcp.enabled": true,
  "godot-mcp.serverPath": "${workspaceFolder}/godot-mcp/build/index.js",
  "godot-mcp.godotPath": "/Applications/Godot.app/Contents/MacOS/Godot",
  "godot-mcp.projectPath": "${workspaceFolder}",
  "godot-mcp.features": {
    "autoSave": true,
    "debugCapture": true,
    "syntaxValidation": true
  },
  "godot-mcp.integration": {
    "vsCodeMcp": true,
    "claudeCode": true,
    "existingServers": {
      "codecmcp": "connected",
      "fetch": "connected", 
      "filesystem": "connected",
      "github": "connected",
      "memory": "connected",
      "puppeteer": "connected",
      "sequential-thinking": "connected",
      "sqlite": "connected"
    }
  }
}