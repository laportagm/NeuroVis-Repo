{
    "// === NEUROVIS VS CODE CONFIGURATION - ENHANCED PROFESSIONAL SETUP ===": "Optimized for educational neuroscience platform development",
    
    "// === GODOT INTEGRATION - ENHANCED ===": "",
    "godot_tools.editor_path": "/Applications/Godot.app/Contents/MacOS/Godot",
    "godot_tools.gdscript_lsp_server_port": 6005,
    "godot_tools.gdscript_lsp_server_host": "127.0.0.1",
    "godot_tools.editor_version": "4.x",
    "godot_tools.scene_file_config": "main",
    "godot_tools.editor_server_enabled": true,
    "godot_tools.connection_retry_count": 5,
    "godot_tools.connection_retry_delay": 1000,
    "godot_tools.show_connection_status": true,
    "godot_tools.check_for_updates": true,
    "godot_tools.export_presets_enabled": true,
    "godot_tools.scene_tree_refresh_interval": 1000,
    "godot_tools.auto_reload_scripts": true,
    "godot_tools.debugger_auto_attach": true,
    "godot_tools.project_files_monitor": ["*.gd", "*.tscn", "*.tres", "*.glb", "*.json"],
    "godot_tools.performance_profiling": true,
    "godot_tools.memory_profiling": true,
    "godot_tools.large_file_threshold": "10MB",
    
    "// === EDITOR BEHAVIOR - ENHANCED ===": "",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.trimAutoWhitespace": true,
    "editor.minimap.enabled": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.inlayHints.enabled": "on",
    "editor.rulers": [80, 100],
    "editor.renderWhitespace": "boundary",
    "editor.showFoldingControls": "always",
    "editor.foldingHighlight": true,
    "editor.occurrencesHighlight": "singleFile",
    "editor.selectionHighlight": true,
    "editor.wordHighlight": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll": "explicit"
    },
    
    "// === FILE HANDLING - ENHANCED ===": "",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        ".godot/": true,
        "**/*.import": true,
        "**/*.uid": true,
        "**/.DS_Store": true,
        "**/logs/**": true,
        "**/tmp/**": true,
        "**/build/**": true,
        "**/export/**": true,
        "**/export_cache/**": true,
        "**/performance_logs/**": true,
        "**/user_data/**": true,
        "**/ai_model_cache/**": true
    },
    "files.associations": {
        "*.gd": "gdscript",
        "*.tscn": "godot-scene",
        "*.tres": "godot-resource",
        "*.godot": "godot-project",
        "*.cs": "csharp",
        "anatomical_data.json": "jsonc",
        "educational_*.json": "jsonc",
        "performance_*.log": "log",
        "*.glb": "binary"
    },
    "files.watcherExclude": {
        ".godot/": true,
        "**/logs/**": true,
        "**/*.import": true,
        "**/tmp/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/export_cache/**": true,
        "**/performance_logs/**": true,
        "**/user_data/**": true,
        "**/ai_model_cache/**": true
    },
    
    "// === GDSCRIPT SPECIFIC - ENHANCED ===": "",
    "[gdscript]": {
        "editor.insertSpaces": false,
        "editor.detectIndentation": false,
        "editor.tabSize": 4,
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 100,
        "editor.suggest.showKeywords": true,
        "editor.suggest.showSnippets": true,
        "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
        "editor.defaultFormatter": "geequlim.godot-tools",
        "editor.semanticHighlighting.enabled": true,
        "editor.suggest.insertMode": "replace"
    },
    
    "// === SEARCH SETTINGS - ENHANCED ===": "",
    "search.exclude": {
        "**/.godot": true,
        "**/build": true,
        "**/export": true,
        "**/.import": true,
        "**/tmp": true,
        "**/*.tmp": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/export_cache": true,
        "**/performance_logs": true,
        "**/ai_model_cache": true
    },
    "search.smartCase": true,
    "search.globalFindClipboard": true,
    
    "// === GIT INTEGRATION - ENHANCED ===": "",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.allowNoVerifyCommit": false,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "git.showProgress": true,
    "scm.defaultViewMode": "tree",
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    
    "// === INTELLISENSE - ENHANCED ===": "",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.suggest.insertMode": "replace",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.suggest.showStatusBar": true,
    "editor.suggest.preview": true,
    "editor.parameterHints.enabled": true,
    "editor.parameterHints.cycle": true,
    
    "// === DEBUGGING - ENHANCED ===": "",
    "debug.allowBreakpointsEverywhere": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.openDebug": "openOnDebugBreak",
    "debug.showInlineBreakpointCandidates": true,
    "debug.showSubSessionsInToolBar": true,
    
    "// === WORKSPACE ORGANIZATION - ENHANCED ===": "",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.gd": "${capture}.gd.remap",
        "*.tscn": "${capture}.tscn.remap, ${capture}.gd",
        "*.tres": "${capture}.tres.remap",
        "project.godot": "*.cfg, *.ini, export_presets.cfg",
        "CLAUDE.md": "README.md, CHANGELOG.md, CONTRIBUTING.md",
        "package.json": "package-lock.json, yarn.lock",
        "anatomical_data.json": "educational_*.json, medical_*.json"
    },
    "explorer.sortOrder": "type",
    "explorer.compactFolders": false,
    
    "// === AI ASSISTANCE - ENHANCED ===": "",
    "github.copilot.enable": {
        "*": true,
        "gdscript": true,
        "godot-scene": true,
        "godot-resource": true,
        "plaintext": true,
        "markdown": true,
        "json": true,
        "yaml": true
    },
    "github.copilot.advanced": true,
    "github.copilot.editor.enableAutoCompletions": true,
    
    "// === GITHUB ACTIONS INTEGRATION - NEW ===": "",
    "githubActions.workflows.pinned": [
        "build.yml",
        "performance-alerts.yml", 
        "test.yml",
        "performance.yml"
    ],
    "githubActions.notifications": {
        "onFailure": "popup",
        "onSuccess": "statusBar",
        "onPerformanceRegression": "popup",
        "onEducationalContentError": "popup"
    },
    "githubActions.statusBar.enabled": true,
    "githubActions.statusBar.showWorkflowName": true,
    "githubActions.statusBar.showBranch": true,
    
    "// === MARKDOWN SETTINGS - ENHANCED ===": "",
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    
    "// === JSON SETTINGS - ENHANCED ===": "",
    "[json]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.detectIndentation": true,
        "editor.defaultFormatter": "ms-vscode.json"
    },
    
    "[jsonc]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.defaultFormatter": "ms-vscode.json"
    },
    
    "// === YAML SETTINGS - NEW ===": "",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    
